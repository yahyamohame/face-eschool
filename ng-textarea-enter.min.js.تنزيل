"use strict"; angular.module("ngTextareaEnter", []).directive("ngTextareaEnter", function () { return { restrict: "A", link: function (a, b, c) { b.bind("keydown", function (d) { var e = d.keyCode || d.which; if (13 === e && "textarea" == b[0].type) { var f = function (a, b, c) { var d, e; for (d = 0, b = b.split("."), e = b.length; d < e; d++) { if (!a || "object" != typeof a) return c; a = a[b[d]] } return void 0 === a ? c : a }, g = f(a, c.ngModel); void 0 !== g && "" !== g && (d.shiftKey || d.ctrlKey || d.altKey || (d.preventDefault(), a.$apply(c.ngTextareaEnter))) } }) } } });